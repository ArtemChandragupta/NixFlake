  pname = "Gruvbox Plus Dark";

  src = fetchFromGitHub {
    owner = "SylEleuth";
    repo  = "gruvbox-plus-icon-pack";
    rev   = "ac1071403164cf3188abcb1e2e4f07c93f39978d";
    hash  = "sha256-KF8/CeQ/zPptBS4hahbs0hiQSnR5SK6pRcWxyJ0LJes=";
  };

  dontBuild = true;

  installPhase = ''
    runHook preInstall
    mkdir -p $out/share/icons/"Gruvbox Plus Dark"
